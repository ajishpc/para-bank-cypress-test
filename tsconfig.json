{
    "compilerOptions": {
        "outDir": "build",
        "sourceMap": false,
        "types": [
            "cypress",
            "node"
        ],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "Node"
    },
    "include": [
        "**/*.ts"
    ]
}